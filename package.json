{
  "name": "blog-0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --turbopack",
    "dev:https": "yarn dev & sleep 3 && local-ssl-proxy --source 3010 --target 3000 --cert generated/localhost.pem --key generated/localhost-key.pem",
    "dev": "next dev --turbopack",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "generate-types": "node scripts/generate-storyblok-types.js",
    "kill-ports": "./scripts/kill-ports.sh",
    "lint": "eslint",
    "proxy": "local-ssl-proxy --source 3010 --target 3000 --cert generated/localhost.pem --key generated/localhost-key.pem",
    "setup-https": "./scripts/setup-https.sh",
    "start": "next start",
    "types": "yarn generate-types"
  },
  "dependencies": {
    "@storyblok/react": "^5.4.14",
    "@storyblok/richtext": "^3.8.2",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "storyblok": "^4.6.9",
    "storyblok-generate-ts": "^2.2.0",
    "typescript": "^5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 80,
    "arrowParens": "always"
  }
}
